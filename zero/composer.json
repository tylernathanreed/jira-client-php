{
    "name": "reedware/openapi-cli",
    "description": "A CLI tool for generating APIs from an OpenAPI Specification.",
    "version": "0.0.1",
    "keywords": [
        "openapi",
        "cli"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Tyler Reed",
            "email": "tylernathanreed@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2.0"
    },
    "require-dev": {
        "laravel-zero/framework": "^11.36.1",
        "laravel/pint": "^1.18.3",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.7.1",
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Jira\\CodeGen\\": "../codegen/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "builds/openapi"
    ]
}
